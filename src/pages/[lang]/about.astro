---
import BaseLayout from "../../layouts/base.astro";

const locale = Astro.currentLocale;

export async function getStaticPaths() {
  return [
    { params: { lang: 'en' } },
    { params: { lang: 'fr' } },
    { params: { lang: 'it' } },
    { params: { lang: 'es' } },
    { params: { lang: 'de' } },
  ];
}
---

<BaseLayout locale={locale}>
    <div id="container" class="container">
        About
    </div>
</BaseLayout>